<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.lehigh.study.smartswitch.fencedpresencepublisher">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="edu.umich.oasis.permission.BIND_OASIS_SERVICE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="edu.umich.oasis.manifest"
            android:resource="@xml/oasis_manifest" />

        <service
            android:name=".PresenceInjector"
            android:enabled="true"
            android:exported="false"></service>
    </application>

</manifest>